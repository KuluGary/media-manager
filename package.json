    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.0",
    "drizzle-zod": "^0.8.3",
    "hono": "^4.10.7",
    "pg": "^8.11.5",
    "postgres": "^3.4.4",
    "stoker": "^2.0.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.13",
    "zod-validation-error": "^5.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.5.1",
