{
  "name": "media-manager",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "tsx watch src/server.ts",
    "build": "tsx ./build.ts",
    "start": "node ./dist/index.js",
    "db:migrate": "tsx src/libs/database/migrate.ts",
