    "@vitest/ui": "^1.6.0",
    "esbuild": "^0.21.2",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.10.2",
    "typescript": "^5.4.5",
